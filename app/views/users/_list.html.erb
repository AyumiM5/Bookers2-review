<h2>Users</h2>
<table class="table">
  <thead>
    <th>image</th>
    <th>name</th>
    <th></th>
  </thead>
  <tbody>
    <% users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :image_id, size: "40x40", fallback: "no_image.jpg" %></td>
      <td><%= user.name %></td>
      <td><%= link_to "show", user_path(user) %></td>
    </tr>
    <% end %>
  </tbody>
</table>